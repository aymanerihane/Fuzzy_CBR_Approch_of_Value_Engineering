// Case Retrieval Flowchart
digraph {
	rankdir=LR
	A [label="New Query Case"]
	B [label="Calculate Membership
to Clusters"]
	C [label="Identify Primary
Cluster"]
	D [label="Retrieve Similar Cases
from Primary Cluster"]
	E [label="Check Similarity
Threshold"]
	F [label="Check Fallback
Clusters"]
	G [label="Final Set of
Similar Cases"]
	A -> B [label=Features]
	B -> C [label=Memberships]
	C -> D
	D -> E
	E -> F [label="If not enough
similar cases"]
	E -> G [label="If enough
similar cases"]
	F -> G [label="Additional
cases"]
}
